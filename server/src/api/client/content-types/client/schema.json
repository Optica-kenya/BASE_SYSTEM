{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullname": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Male",
        "Female"
      ]
    },
    "date_of_birth": {
      "type": "date",
      "required": true
    },
    "address": {
      "type": "string"
    },
    "notes": {
      "type": "text"
    },
    "prescriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::prescription.prescription",
      "mappedBy": "client"
    }
  }
}
