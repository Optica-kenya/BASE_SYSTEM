    },
    "right_axis": {
      "type": "integer",
      "required": true
    },
    "right_sphere": {
      "type": "decimal",
      "required": true
    },
    "left_sphere": {
      "type": "decimal",
      "required": true
    },
    "left_cylinder": {
      "type": "decimal",
      "required": true
    },
    "left_axis": {
      "type": "integer"
    },
    "add_power": {
      "type": "decimal",
      "required": true
    },
    "pupillary_distance": {
      "type": "decimal",
      "required": true
    },
    "diagnosis": {
      "type": "string"
    },
    "prescription_date": {
      "type": "date",
      "required": true
    },
    "attachments": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "prescribed_by": {
      "type": "relation",
